{
  "name": "notam-parser",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "parse": "ts-node src/index.ts",
    "parse-debug": "npm run parse -- -d 2025-09-11 -i LLLL",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "setup-hooks": "bash scripts/setup-pre-commit.sh",
    "dev:web": "cd web && npm run dev",
    "build:web": "cd web && npm run build",
    "build:all": "npm run build && npm run build:web",
    "install:web": "cd web && npm install",
    "playwright:install": "playwright install chromium",
    "postinstall": "npm run playwright:install"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.10.0",
    "jest": "^30.1.3",
    "playwright": "^1.40.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2"
  }
}
